    transform_param {
        mirror: true
        crop_size: 224
        mean_value: 104
        mean_value: 117
        mean_value: 123
   }
    data_param {
    source: "/home/imagenet_data/ilsvrc12_train_lmdb"
    batch_size: 32
    backend: LMDB
    }
}
